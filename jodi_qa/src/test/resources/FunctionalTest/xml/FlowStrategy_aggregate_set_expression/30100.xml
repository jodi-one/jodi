<?xml version="1.0" encoding="UTF-8"?>
<Transformation
	xsi:noNamespaceSchemaLocation="../../../../../../../jodi_core/src/main/resources/jodi-model.v1.1.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Name>FLOWSTRATEGY_AGGREGATE_SET_EXPRESSION</Name>
	<Comments>
	</Comments>
	<PackageList>FLOWSTRATEGY_AGGREGATE_SET_EXPRESSION</PackageList>
	<Datasets>
		<Dataset>
			<Source>
				<Name>S_DA_I</Name>
		    	<Filter>S_DA_I.KEY='KEY_1'</Filter>
			</Source>
			<Source>
				<Name>S_DB_I</Name>
				<Alias>DB</Alias>
				<Join>S_DA_I.KEY = DB.KEY</Join>
				<JoinType>LEFT OUTER</JoinType>
			</Source>
		</Dataset>
		<Dataset>
			<Source>
				<Name>S_DA_I</Name>
			</Source>
			<Source>
				<Name>S_DB_I</Name>
				<Alias>DB</Alias>
				<Join>S_DA_I.KEY = DB.KEY</Join>
				<JoinType>LEFT OUTER</JoinType>
				<Filter>DB.KEY='KEY_1'</Filter>
			</Source>
		</Dataset>
	</Datasets>
	<Mappings>
	    <Distinct>true</Distinct>
		<TargetDataStore>S_DA_O</TargetDataStore>
		<TargetColumn>
			<Name>KEY</Name>
			<MappingExpressions>
				<Expression>S_DA_I.KEY</Expression>
				<Expression>S_DA_I.KEY</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>VALUE</Name>
			<MappingExpressions>
				<Expression>SUM(2)</Expression>
				<Expression>S_DA_I.VALUE + 1</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>LAST_CHANGED_DT</Name>
			<MappingExpressions>
				<Expression>DB.LAST_CHANGED_DT</Expression>
				<Expression>DB.LAST_CHANGED_DT</Expression>
			</MappingExpressions>
		</TargetColumn>
	</Mappings>
</Transformation>
