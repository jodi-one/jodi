<?xml version="1.0" encoding="UTF-8"?>
<Transformation
	xsi:noNamespaceSchemaLocation="../../../../../../../jodi_core/src/main/resources/jodi-model.v1.1.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Comments>
	</Comments>
	<PackageList>FlowStrategy_TargetExpressions</PackageList>
	<Name>FlowStrategy_TargetExpressions</Name>
	<Datasets>
		<Dataset>
			<Source>
				<Name>S_DA_I</Name>
				<Alias>DA_1</Alias>
			<Lookups> 
			<Lookup>
				<LookupDataStore>S_DB_I</LookupDataStore>
				<Alias>DB_1</Alias>
				<Join>DA_1.KEY = DB_1.KEY</Join>
			</Lookup>
			</Lookups>	
			</Source>
		</Dataset>
		<Dataset>
			<Source>
				<Name>S_DA_I</Name>
				<Alias>DA_2</Alias>
			</Source>
		</Dataset>
	</Datasets>
	<Mappings>
		<TargetDataStore>S_DA_O</TargetDataStore>
		<TargetColumn>
			<Name>KEY</Name>
			<MappingExpressions>
				<Expression>DA_1.KEY</Expression>
				<Expression>DA_2.KEY</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>VALUE</Name>
			<MappingExpressions>
				<Expression>DA_1.VALUE</Expression>
				<Expression>DA_2.VALUE</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>LAST_CHANGED_DT</Name>
			<MappingExpressions>
				<Expression>DB_1.LAST_CHANGED_DT</Expression>
				<Expression>DA_2.LAST_CHANGED_DT</Expression>
			</MappingExpressions>
		</TargetColumn>
	</Mappings>
</Transformation>