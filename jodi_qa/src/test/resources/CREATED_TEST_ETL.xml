<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Loadplan xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="../../../../jodi_core/src/main/resources/jodi-loadplan.v1.0.xsd">
    <Name>CREATED_TEST_FILE</Name>
    <Folder>CREATED_TEST_FILE</Folder>
    <Exceptions>
        <Exception>
            <Children>
                <RunScenario>
                    <Variables>
                        <Variable>
                            <Name>GLOBAL.GLOBAL_VAR_PARTITION_NAME</Name>
                            <Refresh>true</Refresh>
                        </Variable>
                    </Variables>
                    <Scenario>INITIALDWH_CON_CHINOOK</Scenario>
                </RunScenario>
            </Children>
        </Exception>
        <Exception>
            <Children>
                <Serial>
                    <Variables>
                        <Variable>
                            <Name>SAMPLEC.VAR_PARTITION_NAME</Name>
                            <Refresh>false</Refresh>
                            <Value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                                   xsi:type="xs:string">TEST VAR Exception
                            </Value>
                        </Variable>
                    </Variables>
                    <Children>
                        <RunScenario>
                            <ExceptionBehavior>NONE</ExceptionBehavior>
                            <Variables>
                                <Variable>
                                    <Name>SAMPLEC.VAR_PARTITION_NAME</Name>
                                    <Refresh>false</Refresh>
                                    <Value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                                           xsi:type="xs:string">TEST VAR Exception RunScenario
                                    </Value>
                                </Variable>
                            </Variables>
                            <Scenario>TEST_MKDIR</Scenario>
                        </RunScenario>
                    </Children>
                </Serial>
                <Case>
                    <TestVariable>SAMPLEC.LOAD_UP_TO_DATE</TestVariable>
                    <When>
                        <Operator>LESS_THAN</Operator>
                        <Value xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:long">1</Value>
                        <Children>
                            <RunScenario>
                                <Scenario>DDL_ORACLE_DWH_CON_CHINOOK_001</Scenario>
                            </RunScenario>
                        </Children>
                    </When>
                    <When>
                        <Name>When Value &lt; 2</Name>
                        <Operator>LESS_THAN</Operator>
                        <Value xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:long">2</Value>
                        <Children>
                            <Parallel>
                                <Variables>
                                    <Variable>
                                        <Name>SAMPLEC.VAR_PARTITION_NAME</Name>
                                        <Refresh>false</Refresh>
                                        <Value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                                               xsi:type="xs:string">TEST VAR Exception Parallel
                                        </Value>
                                    </Variable>
                                </Variables>
                                <Children>
                                    <RunScenario>
                                        <Scenario>REALTIMEDWH_DMT_FACT</Scenario>
                                    </RunScenario>
                                </Children>
                            </Parallel>
                        </Children>
                    </When>
                    <Else>
                        <Children>
                            <RunScenario>
                                <Scenario>TEST_MKDIR</Scenario>
                            </RunScenario>
                        </Children>
                    </Else>
                </Case>
            </Children>
        </Exception>
    </Exceptions>
    <Serial>
        <Variables>
            <Variable>
                <Name>SAMPLEC.VAR_PARTITION_NAME</Name>
                <Value xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">TEST VAR SERIAL</Value>
            </Variable>
        </Variables>
        <Children>
            <RunScenario>
                <Variables>
                    <Variable>
                        <Name>SAMPLEC.VAR_PARTITION_NAME</Name>
                        <Value xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">TEST VAR RUN SCENARIO
                        </Value>
                    </Variable>
                </Variables>
                <Scenario>DROP_TEMP_INTERFACE_DATASTORES</Scenario>
            </RunScenario>
            <Case>
                <Variables>
                    <Variable>
                        <Name>SAMPLEC.VAR_PARTITION_NAME</Name>
                        <Refresh>false</Refresh>
                        <Value xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">TEST VAR RUN CASE
                        </Value>
                    </Variable>
                </Variables>
                <TestVariable>SAMPLEC.LOAD_UP_TO_DATE</TestVariable>
                <When>
                    <Operator>IS_NULL</Operator>
                    <Children>
                        <RunScenario>
                            <Scenario>TEST_MKDIR</Scenario>
                        </RunScenario>
                        <RunScenario>
                            <Scenario>DISABLE_CONSTRAINTS</Scenario>
                        </RunScenario>
                    </Children>
                </When>
                <When>
                    <Operator>GREATER_THAN</Operator>
                    <Value xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:long">0</Value>
                    <Children>
                        <RunScenario>
                            <Name>DISABLE_CONSTRAINTS2</Name>
                            <Scenario>DISABLE_CONSTRAINTS</Scenario>
                        </RunScenario>
                    </Children>
                </When>
            </Case>
            <RunScenario>
                <ExceptionBehavior>RUN_EXCEPTION_AND_IGNORE_ERROR</ExceptionBehavior>
                <Scenario>INITIALDWH_CON_CHINOOK</Scenario>
            </RunScenario>
            <RunScenario>
                <ExceptionBehavior>RUN_EXCEPTION_AND_IGNORE_ERROR</ExceptionBehavior>
                <Scenario>INITIALDWH_CON</Scenario>
            </RunScenario>
            <RunScenario>
                <Scenario>INITIALDWH_OIL</Scenario>
            </RunScenario>
            <RunScenario>
                <Scenario>INITIALDWH_STI</Scenario>
            </RunScenario>
            <RunScenario>
                <Scenario>INITIALDWH_STO</Scenario>
            </RunScenario>
            <Parallel>
                <Variables>
                    <Variable>
                        <Name>SAMPLEC.VAR_PARTITION_NAME</Name>
                        <Refresh>false</Refresh>
                        <Value xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:type="xs:string">TEST VAR PARALLEL
                        </Value>
                    </Variable>
                </Variables>
                <Children>
                    <RunScenario>
                        <Scenario>DISABLE_BITMAP_INDEX</Scenario>
                    </RunScenario>
                </Children>
            </Parallel>
            <RunScenario>
                <Scenario>INITIALDWH_DMT</Scenario>
            </RunScenario>
            <RunScenario>
                <Scenario>INITIALDWH_DMT_FACT</Scenario>
            </RunScenario>
        </Children>
    </Serial>
</Loadplan>
