<?xml version="1.0" encoding="UTF-8"?>
<Transformation
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../jodi_core/src/main/resources/jodi-model.v1.1.xsd">
    <Comments></Comments>
    <PackageList>PACKAGE_ALL</PackageList>
    <Datasets>
        <Dataset>
            <Source>
                <Name>O_INVOICE</Name>
            </Source>
        </Dataset>
    </Datasets>
    <Mappings>
        <TargetDataStore>S_INVOICE_I</TargetDataStore>
        <TargetColumn>
            <Name>INVE_CODE</Name>
            <MappingExpressions>
                <Expression>O_INVOICE.INVOICE_CODE</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>INVE_CUSTOMER_CODE</Name>
            <MappingExpressions>
                <Expression>O_INVOICE.CUSTOMER_CODE</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>INVE_DATE_CODE</Name>
            <MappingExpressions>
                <Expression>CAST( TO_CHAR(O_INVOICE.INVOICE_DATE,'YYYYMMDD') AS INTEGER)</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>INVE_TOTAL</Name>
            <MappingExpressions>
                <Expression>O_INVOICE.TOTAL</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>S_INSERT_DT_I</Name>
            <MappingExpressions>
                <Expression>SYSDATE</Expression>
            </MappingExpressions>
        </TargetColumn>
    </Mappings>
</Transformation>