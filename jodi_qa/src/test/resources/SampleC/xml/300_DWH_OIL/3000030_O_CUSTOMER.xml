<?xml version="1.0" encoding="UTF-8"?>
<Transformation
	xsi:noNamespaceSchemaLocation="../../../../../../../jodi_core/src/main/resources/jodi-model.v1.1.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Comments></Comments>
	<PackageList>PACKAGE_ALL</PackageList>
	<Datasets>
		<Dataset>
			<Source>
				<Name>CHIN_CUSTOMER</Name>
				<Lookups>
					<Lookup>
						<LookupDataStore>O_CUSTOMER_S01</LookupDataStore>
						<Alias>CHIN_INVOICE</Alias>
						<Join>CHIN_INVOICE.LAST_INVOICE_CODE_CUSTOMER = CHIN_INVOICE.INVOICE_CODE AND CHIN_CUSTOMER.CUSTOMER_CODE = CHIN_INVOICE.CUSTOMER_CODE</Join>
						<!--  auto set for analytical functions
						<SubSelect>true</SubSelect>
						 -->
					</Lookup>
				</Lookups>
			</Source>
		</Dataset>
	</Datasets>
	<Mappings>
		<TargetDataStore>O_CUSTOMER</TargetDataStore>
		<TargetColumn>
			<Name>CUSTOMER_CODE</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.CUSTOMER_CODE</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>FIRSTNAME</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.FIRSTNAME</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>LASTNAME</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.LASTNAME</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>COMPANY</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.COMPANY</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>ADDRESS</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.ADDRESS</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>CITY</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.CITY</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>STATE</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.STATE</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>COUNTRY</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.COUNTRY</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>POSTALCODE</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.POSTALCODE</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>PHONE</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.PHONE</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>FAX</Name>
			<MappingExpressions>
				<Expression>CHIN_CUSTOMER.FAX</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>SUPPORTREP_CODE</Name>
			<MappingExpressions>
				<Expression>NVL(CHIN_CUSTOMER.SUPPORTREP_CODE,'UNKNOWN')</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>EMAIL</Name>
			<MappingExpressions>
				<Expression>NVL(CHIN_CUSTOMER.EMAIL,'UNKNOWN')</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_ADDRESS</Name>
			<MappingExpressions>
				<Expression>CHIN_INVOICE.BILLING_ADDRESS</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_CITY</Name>
			<MappingExpressions>
				<Expression>CHIN_INVOICE.BILLING_CITY</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_STATE</Name>
			<MappingExpressions>
				<Expression>CHIN_INVOICE.BILLING_STATE</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_COUNTRY</Name>
			<MappingExpressions>
				<Expression>CHIN_INVOICE.BILLING_COUNTRY</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_POSTALCODE</Name>
			<MappingExpressions>
				<Expression>CHIN_INVOICE.BILLING_POSTALCODE</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>O_INSERT_DATE</Name>
			<MappingExpressions>
				<Expression>SYSDATE</Expression>
			</MappingExpressions>
		</TargetColumn>
	</Mappings>
</Transformation>