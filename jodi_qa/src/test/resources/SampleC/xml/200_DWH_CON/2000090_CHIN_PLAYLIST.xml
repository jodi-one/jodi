<?xml version="1.0" encoding="UTF-8"?>
<Transformation
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../jodi_core/src/main/resources/jodi-model.v1.1.xsd">
    <Comments></Comments>
    <PackageList>PACKAGE_ALL</PackageList>
    <Datasets>
        <Dataset>
            <Source>
                <Name>PLAYLIST</Name>
                <Alias>PLAYLIST</Alias>
            </Source>
        </Dataset>
    </Datasets>
    <Mappings>
        <TargetDataStore>CHIN_PLAYLIST</TargetDataStore>
        <TargetColumn>
            <Name>PLAYLIST_CODE</Name>
            <MappingExpressions>
                <Expression>'CHIN|'||PLAYLIST.PLAYLISTID</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>NAME</Name>
            <MappingExpressions>
                <Expression>PLAYLIST.NAME</Expression>
            </MappingExpressions>
        </TargetColumn>
    </Mappings>
</Transformation>