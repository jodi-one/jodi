<?xml version="1.0" encoding="UTF-8"?>
<Transformation
	xsi:noNamespaceSchemaLocation="../../../../../../../jodi_core/src/main/resources/jodi-model.v1.1.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Comments></Comments>
	<PackageList>PACKAGE_ALL</PackageList>
	<Datasets>
		<Dataset>
			<Model>model.edw_sds.code</Model>
			<Source>
				<Name>INVOICE</Name>
				<Alias>INVE</Alias>
				<Lookups>
					<Lookup>
						<LookupDataStore>CUSTOMER</LookupDataStore>
						<Alias>CUST</Alias>
						<Join>INVE.CUSTOMERID = CUST.CUSTOMERID</Join>
					</Lookup>
				</Lookups>
			</Source>
		</Dataset>
	</Datasets>
	<Mappings>
		<TargetDataStore>CHIN_CUSTOMER_S01</TargetDataStore>
		<TargetColumn>
			<Name>CUSTOMER_CODE</Name>
			<MappingExpressions>
				<Expression>CUST.EMAIL</Expression>
			</MappingExpressions>
			<Properties>
				<DataType>VARCHAR2</DataType>
				<Length>60</Length>
			</Properties>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_ADDRESS</Name>
			<MappingExpressions>
				<Expression>INVE.BILLINGADDRESS</Expression>
			</MappingExpressions>
			<Properties>
				<DataType>VARCHAR2</DataType>
				<Length>70</Length>
			</Properties>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_CITY</Name>
			<MappingExpressions>
				<Expression>INVE.BILLINGCITY</Expression>
			</MappingExpressions>
			<Properties>
				<DataType>VARCHAR2</DataType>
				<Length>40</Length>
			</Properties>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_STATE</Name>
			<MappingExpressions>
				<Expression>INVE.BILLINGSTATE</Expression>
			</MappingExpressions>
			<Properties>
				<DataType>VARCHAR2</DataType>
				<Length>40</Length>
			</Properties>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_COUNTRY</Name>
			<MappingExpressions>
				<Expression>INVE.BILLINGCOUNTRY</Expression>
			</MappingExpressions>
			<Properties>
				<DataType>VARCHAR2</DataType>
				<Length>40</Length>
			</Properties>
		</TargetColumn>
		<TargetColumn>
			<Name>BILLING_POSTALCODE</Name>
			<MappingExpressions>
				<Expression>INVE.BILLINGPOSTALCODE</Expression>
			</MappingExpressions>
			<Properties>
				<DataType>VARCHAR2</DataType>
				<Length>10</Length>
			</Properties>
		</TargetColumn>
		<TargetColumn>
			<Name>RANK</Name>
			<MappingExpressions>
				<Expression>COUNT(distinct INVE.BILLINGPOSTALCODE )</Expression>
			</MappingExpressions>
			<Properties>
				<DataType>NUMBER</DataType>
				<Length>10</Length>
			</Properties>
		</TargetColumn>
	</Mappings>
</Transformation>