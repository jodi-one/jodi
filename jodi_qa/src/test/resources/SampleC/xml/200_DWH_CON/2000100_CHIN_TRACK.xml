<?xml version="1.0" encoding="UTF-8"?>
<Transformation
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../jodi_core/src/main/resources/jodi-model.v1.1.xsd">
    <Comments></Comments>
    <PackageList>PACKAGE_ALL</PackageList>
    <Datasets>
        <Dataset>
            <Source>
                <Name>CHIN_TRACK_S01</Name>
                <Alias>TRACK</Alias>
            </Source>
            <Source>
                <Name>CHIN_ALBUM</Name>
                <Alias>CHIN_ALBUM</Alias>
                <Join>TRACK.ALBUMID = CHIN_ALBUM.ALBUMID</Join>
                <JoinType>INNER</JoinType>
            </Source>
            <Source>
                <Name>CHIN_MEDIATYPE</Name>
                <Alias>MEDIATYPE</Alias>
                <Join>TRACK.MEDIATYPEID = MEDIATYPE.MEDIATYPEID</Join>
                <JoinType>INNER</JoinType>
            </Source>
            <Source>
                <Name>CHIN_GENRE</Name>
                <Alias>GENRE</Alias>
                <Join>TRACK.GENREID = GENRE.GENREID</Join>
                <JoinType>INNER</JoinType>
            </Source>
        </Dataset>
    </Datasets>
    <Mappings>
        <TargetDataStore>CHIN_TRACK</TargetDataStore>
        <TargetColumn>
            <Name>TRACKID</Name>
            <MappingExpressions>
                <Expression>TRACK.TRACKID</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>TRACK_CODE</Name>
            <MappingExpressions>
                <Expression>'CHIN|'||TRACK.TRACKID</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>NAME</Name>
            <MappingExpressions>
                <Expression>TRACK.NAME</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>ALBUM_CODE</Name>
            <MappingExpressions>
                <Expression>CHIN_ALBUM.ALBUM_CODE</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>MEDIATYPE</Name>
            <MappingExpressions>
                <Expression>MEDIATYPE.NAME</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>GENRE</Name>
            <MappingExpressions>
                <Expression>GENRE.NAME</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>COMPOSER</Name>
            <MappingExpressions>
                <Expression>TRACK.COMPOSER</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>MILISECONDS</Name>
            <MappingExpressions>
                <Expression>TRACK.MILLISECONDS</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>BYTES</Name>
            <MappingExpressions>
                <Expression>TRACK.BYTES</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>UNITPRICE</Name>
            <MappingExpressions>
                <Expression>TRACK.UNITPRICE</Expression>
            </MappingExpressions>
        </TargetColumn>
    </Mappings>
</Transformation>