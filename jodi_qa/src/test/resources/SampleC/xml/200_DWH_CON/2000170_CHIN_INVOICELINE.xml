<?xml version="1.0" encoding="UTF-8"?>
<Transformation
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../jodi_core/src/main/resources/jodi-model.v1.1.xsd">
    <Comments></Comments>
    <PackageList>PACKAGE_ALL</PackageList>
    <Datasets>
        <Dataset>
            <Source>
                <Name>INVOICELINE</Name>
                <Alias>INVOICELINE</Alias>
            </Source>
            <Source>
                <Name>CHIN_TRACK</Name>
                <Join>CHIN_TRACK.TRACKID = INVOICELINE.TRACKID</Join>
            </Source>
        </Dataset>
    </Datasets>
    <Mappings>
        <TargetDataStore>CHIN_INVOICELINE</TargetDataStore>
        <TargetColumn>
            <Name>INVOICE_LINE_CODE</Name>
            <MappingExpressions>
                <Expression>'CHIN|'||INVOICELINE.INVOICEID||'|'||INVOICELINE.INVOICELINEID</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>INVOICE_CODE</Name>
            <MappingExpressions>
                <Expression>'CHIN|'||INVOICELINE.INVOICEID</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>TRACK_CODE</Name>
            <MappingExpressions>
                <Expression>CHIN_TRACK.TRACK_CODE</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>UNITPRICE</Name>
            <MappingExpressions>
                <Expression>INVOICELINE.UNITPRICE</Expression>
            </MappingExpressions>
        </TargetColumn>
        <TargetColumn>
            <Name>QUANTITY</Name>
            <MappingExpressions>
                <Expression>INVOICELINE.QUANTITY</Expression>
            </MappingExpressions>
        </TargetColumn>
    </Mappings>
</Transformation>