	</Datasets>
	<Mappings>
		<TargetDataStore>CHIN_PLAYLISTTRACK</TargetDataStore>
		<TargetColumn>
			<Name>PLAYLIST_CODE</Name>
			<MappingExpressions>
				<Expression>'CHIN|'||PLAYLISTTRACK.PLAYLISTID</Expression>
			</MappingExpressions>
		</TargetColumn>
		<TargetColumn>
			<Name>TRACK_CODE</Name>
			<MappingExpressions>
				<Expression>CHIN_TRACK.TRACK_CODE</Expression>
			</MappingExpressions>
		</TargetColumn>
	</Mappings>
</Transformation>